{
  "expo": {
    "name": "StudentVerse",
    "slug": "studentverse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "studentverse",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#080C1F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studentverse.app",
      "splash": {
        "image": "./assets/images/splash-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#080C1F"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#080C1F",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "splash": {
        "image": "./assets/images/splash-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#080C1F"
      },
      "package": "com.studentverse.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-logo.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#080C1F"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/Inter_400Regular.ttf",
            "node_modules/@expo-google-fonts/inter/Inter_500Medium.ttf",
            "node_modules/@expo-google-fonts/inter/Inter_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/inter/Inter_700Bold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
